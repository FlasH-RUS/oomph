  <product name="eclipse"
      label="Eclipse for Java">
    <annotation
        source="http://www.eclipse.org/oomph/setup/BrandingInfo">
      <detail
          key="folderName">
        <value>eclipse</value>
      </detail>
      <detail
          key="folderName.macosx">
        <value>Eclipse</value>
      </detail>
      <detail
          key="siteURI">
        <value>https://github.com/FlasH-RUS/oomph</value>
      </detail>
