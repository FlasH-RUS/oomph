    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/jdt/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/JDT.ecore http://www.eclipse.org/oomph/setup/workbench/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Workbench.ecore"
    name="ru.lonedeveloper.flash.products"
    label="FlasH's products">
  <setupTask
      xsi:type="setup:CompoundTask"
      name="Dark theme">
    <annotation
        source="http://www.eclipse.org/oomph/setup/UserPreferences"/>
    <setupTask
        xsi:type="setup:CompoundTask"
        name="org.eclipse.e4.ui.css.swt.theme">
      <setupTask
          xsi:type="setup:PreferenceTask"
          key="/instance/org.eclipse.e4.ui.css.swt.theme/themeid"
          value="org.eclipse.e4.ui.css.theme.e4_dark"/>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        name="org.eclipse.e4.ui.workbench.renderers.swt">
      <setupTask
          xsi:type="setup:PreferenceTask"
          key="/instance/org.eclipse.e4.ui.workbench.renderers.swt/USE_ROUND_TABS"
          value="true"/>
    </setupTask>
    <description>Preferences recorder saves this many settings for selecting a Dark theme :shrug:</description>
  </setupTask>
  <setupTask
      xsi:type="setup:InstallationTask"
      id="installation"/>
  <setupTask
      xsi:type="setup.p2:P2Task">
    <requirement
        name="org.eclipse.oomph.setup.feature.group"/>
    <repository
        url="${oomph.update.url}"/>
  </setupTask>
  <product name="eclipse"
      label="Eclipse for Java">
    <annotation
        source="http://www.eclipse.org/oomph/setup/BrandingInfo">
      <detail
          key="folderName">
        <value>eclipse</value>
      </detail>
      <detail
          key="folderName.macosx">
        <value>Eclipse</value>
      </detail>
      <detail
          key="siteURI">
        <value>https://github.com/FlasH-RUS/oomph</value>
      </detail>
    </annotation>
    <setupTask
        xsi:type="setup:EclipseIniTask"
        option="-Xmx"
        value="2g"
        vm="true"/>
    <setupTask
        xsi:type="setup:CompoundTask"
        name="Lombok">
      <setupTask
          xsi:type="setup:VariableTask"
          type="FILE"
          name="lombok.jar.location"
          label="Lombok JAR location">
        <description>Location of lombok.jar (if not available locally download it from https://projectlombok.org/download.html and place somewhere C:/Data)</description>
      </setupTask>
      <setupTask
          xsi:type="setup:EclipseIniTask"
          option="-javaagent"
          value=":${lombok.jar.location}"
          vm="true"/>
    </setupTask>
    <setupTask
        xsi:type="jdt:JRETask"
        version="JavaSE-15"
        location="${jre.location-15}"/>
    <setupTask
        xsi:type="setup.p2:P2Task"
        label="AutoDeriv">
      <requirement
          name="net.nodj.AutoDerivFeature.feature.group"/>
      <repository
          url="http://nodj.github.io/AutoDeriv/update"/>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        name="MoreUnit">
      <setupTask
          xsi:type="setup.p2:P2Task"
          label="MoreUnit">
        <requirement
            name="org.moreunit.feature.group"/>
        <repository
            url="https://moreunit.github.io/eclipse-update-site/"/>
      </setupTask>
      <setupTask
          xsi:type="workbench:KeyBindingTask"
          filter="(use.idea.keymap=false)"
          keys="CTRL+SHIFT+T"
          command="org.moreunit.core.commands.jumpCommand">
        <context
            id="org.eclipse.jdt.ui.javaEditorScope"/>
        <context
            id="org.eclipse.ui.textEditorScope"/>
        <context/>
      </setupTask>
      <setupTask
          xsi:type="workbench:KeyBindingTask"
          predecessor="//@products[name='eclipse']/@setupTasks.7"
          filter="(use.idea.keymap=true)"
          scheme="IntelliJIdeaKeymap4EclipseScheme"
          keys="CTRL+SHIFT+T"
          command="org.moreunit.core.commands.jumpCommand">
        <context
            id="org.eclipse.jdt.ui.javaEditorScope"/>
        <context
            id="org.eclipse.ui.textEditorScope"/>
        <context/>
      </setupTask>
    </setupTask>
    <setupTask
        xsi:type="setup.p2:P2Task"
        filter=""
        label="Grep console">
      <requirement
          name="name.schedenig.eclipse.grepconsole.feature.group"/>
      <repository
          url="http://eclipse.schedenig.name"/>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        type="BOOLEAN"
        name="use.idea.keymap"
        defaultValue="false"
        label="Use IDEA keymap">
      <description>Do you want to use keyboard shortcuts close to IDEA's?</description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        filter="(use.idea.keymap=true)"
        name="Idea keymap">
      <setupTask
          xsi:type="setup.p2:P2Task"
          label="IntelliJIdeaKeymap4Eclipse">
        <requirement
            name="com.github.intellijideakeymap4eclipse.feature.feature.group"/>
        <repository
            url="https://raw.githubusercontent.com/IntelliJIdeaKeymap4Eclipse/IntelliJIdeaKeymap4Eclipse-update-site/main"/>
      </setupTask>
      <setupTask
          xsi:type="workbench:KeyBindingTask"
          scheme="IntelliJIdeaKeymap4EclipseScheme"
          keys="CTRL+F11"
          command="org.eclipse.ui.window.nextPerspective">
        <context/>
      </setupTask>
      <setupTask
          xsi:type="workbench:KeyBindingTask"
          scheme="IntelliJIdeaKeymap4EclipseScheme"
          keys="CTRL+SHIFT+F11"
          command="org.eclipse.ui.window.nextPerspective">
        <context/>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          name="User preferences">
        <annotation
            source="http://www.eclipse.org/oomph/setup/UserPreferences"/>
        <setupTask
            xsi:type="setup:CompoundTask"
            predecessor="//@products[name='eclipse']/@setupTasks.7/@setupTasks.0"
            name="org.eclipse.ui">
          <setupTask
              xsi:type="setup:PreferenceTask"
              key="/instance/org.eclipse.ui/KEY_CONFIGURATION_ID"
              value="IntelliJIdeaKeymap4EclipseScheme"/>
          <description>IDEA keymap</description>
        </setupTask>
      </setupTask>
      <description>Uses https://github.com/IntelliJIdeaKeymap4Eclipse/IntelliJIdeaKeymap4Eclipse as a provider</description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        name="User preferences">
      <annotation
          source="http://www.eclipse.org/oomph/setup/UserPreferences"/>
      <setupTask
          xsi:type="setup:CompoundTask"
          name="org.eclipse.debug.ui">
        <setupTask
            xsi:type="setup:PreferenceTask"
            key="/instance/org.eclipse.debug.ui/org.eclipse.debug.ui.RelaunchAndTerminateLaunchAction"
            value="true"/>
        <setupTask
            xsi:type="setup:PreferenceTask"
            key="/instance/org.eclipse.debug.ui/org.eclipse.debug.ui.UseContextualLaunch"
            value="true"/>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          name="org.eclipse.ui.ide">
        <setupTask
            xsi:type="setup:PreferenceTask"
            key="/instance/org.eclipse.ui.ide/SAVE_ALL_BEFORE_BUILD"
            value="true"/>
      </setupTask>
    </setupTask>
