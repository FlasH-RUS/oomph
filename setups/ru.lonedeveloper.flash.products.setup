    <setupTask
        xsi:type="setup:EclipseIniTask"
        option="-Xmx"
        value="2g"
        vm="true"/>
    <setupTask
        xsi:type="setup:CompoundTask"
        name="Lombok">
      <setupTask
          xsi:type="setup:VariableTask"
          type="FILE"
          name="lombok.jar.location"
          label="Lombok JAR location">
        <description>Location of lombok.jar (if not available locally download it from https://projectlombok.org/download.html and place somewhere C:/Data)</description>
      </setupTask>
      <setupTask
          xsi:type="setup:EclipseIniTask"
          option="-javaagent"
          value=":${lombok.jar.location}"
          vm="true"/>
    </setupTask>
    <setupTask
        xsi:type="jdt:JRETask"
        version="JavaSE-1.8"
        location="${jre.location-1.8}"/>
    <setupTask
        xsi:type="setup.p2:P2Task"
        label="DevStyle">
      <requirement
          name="com.genuitec.eclipse.theming.feature.feature.group"/>
      <repository
          url="https://www.genuitec.com/updates/devstyle/ci/"/>
    </setupTask>
    <setupTask
        xsi:type="setup.p2:P2Task"
        label="AutoDeriv">
      <requirement
          name="net.nodj.AutoDerivFeature.feature.group"/>
      <repository
          url="http://nodj.github.io/AutoDeriv/update"/>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        name="MoreUnit">
      <setupTask
          xsi:type="setup.p2:P2Task"
          label="MoreUnit">
        <requirement
            name="org.moreunit.feature.group"/>
        <repository
            url="https://moreunit.github.io/eclipse-update-site/"/>
      </setupTask>
      <setupTask
          xsi:type="workbench:KeyBindingTask"
          filter="(use.idea.keymap=false)"
          keys="CTRL+SHIFT+T"
          command="org.moreunit.core.commands.jumpCommand">
        <context
            id="org.eclipse.jdt.ui.javaEditorScope"/>
        <context
            id="org.eclipse.ui.textEditorScope"/>
        <context/>
      </setupTask>
      <setupTask
          xsi:type="workbench:KeyBindingTask"
          predecessor="//@products[name='eclipse']/@setupTasks.8"
          filter="(use.idea.keymap=true)"
          scheme="IntelliJIdeaKeymap4EclipseScheme"
          keys="CTRL+SHIFT+T"
          command="org.moreunit.core.commands.jumpCommand">
        <context
            id="org.eclipse.jdt.ui.javaEditorScope"/>
        <context
            id="org.eclipse.ui.textEditorScope"/>
        <context/>
      </setupTask>
    </setupTask>
